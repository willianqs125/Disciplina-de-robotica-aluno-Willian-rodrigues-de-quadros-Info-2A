// Example program
#include <iostream>
using namespace std;

int main()
{
    int num1 = 10, num4= 3;
    float num2 = 10, num5= 3;
    double num3 = 10, num6 = 3;
    
    cout<< "int "<< num1/num4<<endl;
    cout<< "float "<< num2/num5<<endl;
    cout<< "double "<< num3/num6<<endl;
    
    return 0;
}
